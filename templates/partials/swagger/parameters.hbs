{{#if parameters}}
<div class="oa-section col-md-6">
  <h4>Parameters</h4>
  <ul class="oa-parameters-list">
    {{#each parameters}}
      {{#if $ref}}
        {{> swagger/parameterRow parameter=(json-schema--resolve-ref $ref) $ref=$ref}}
      {{else}}
        {{> swagger/parameterRow parameter=.}}
      {{/if}}
    {{/each}}
  </ul>
</div>
{{/if}}
